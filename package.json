{
  "name": "ZenobiusJiricek-Person-Website",
  "version": "2.0.0",
  "homepage": "https://github.com/airtonix/zenobi.us/",
  "description": "My personal website",
  "keywords": [
    "static",
    "website",
    "github-pages"
  ],
  "author": "Zenobius Jiricek",
  "license": "All rights reserved",
  "bugs": {
    "url": "https://github.com/airtonix/epochdb/issues"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/airtonix/zenobi.us.git"
  },
  "main": "app.coffee",
  "browserify": {
    "extensions": [
      ".coffee",
      ".html"
    ],
    "transform": [
      "coffeeify",
      "browserify-shim"
    ]
  },
  "browserify-shim": {},
  "browser": {},
  "napa": {},
  "scripts": {
    "config": "gastropod list config --config=./config/development",
    "start": "gastropod run default --config=./config/development",
    "build": "gastropod run build --config=./config/production",
    "merge-to-master": "git checkout master && git merge develop",
    "merge-to-develop": "git checkout develop && git merge master",
    "push": "git push --all && git push --tags",
    "preversion": "npm run merge-to-master",
    "postversion": "npm run merge-to-develop && npm run push"
  },
  "devDependencies": {},
  "dependencies": {
    "bourbon": "^4.2.5",
    "bourbon-neat": "^1.7.2",
    "browserify": "^11.2.0",
    "browserify-shim": "^3.8.10",
    "coffeeify": "^1.1.0",
    "gastropod": "0.0.x",
    "gastropod-task-webpack": "^0.0.1",
    "lodash": "^3.10.1",
    "neat": "^1.2.2"
  }
}
