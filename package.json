{
  "name": "zenobi.us",
  "description": "A Vue.js project",
  "version": "2.0.0-beta",
  "homepage": "http://github.com/airtonix/zenobi.us/",
  "author": "Zenobius Jiricek <airtonix@gmail.com>",
  "license": "MIT",
  "private": true,
  "keywords": [
    "Vue",
    "vuex"
  ],
  "scripts": {
    "unit": "karma start test/unit/karma.conf.js",
    "test": "cross-env WEBPACK_ENV=test npm run unit",
    "prebuild": "rimraf ./dist && mkdirp dist",
    "build": "cross-env WEBPACK_ENV=prod babel-node ./build",
    "dev:run": "cross-env DEBUG=app* WEBPACK_ENV=dev babel-node ./build/server",
    "dev": "onchange \"package.json\" \"{mocks,build}/**/*\" -i -v -- npm run dev:run",
    "release": "foundry release",
    "release-resume": "foundry resume"
  },
  "babel": {
    "presets": [
      "es2015",
      "es2016",
      "es2017"
    ],
    "plugins": [
      "transform-runtime",
      "transform-flow-comments",
      "transform-object-rest-spread",
      "syntax-flow"
    ]
  },
  "foundry": {
    "releaseCommands": [
      {
        "type": "customCommand",
        "updateFiles": "git clone git@github.com:airtonix/zenobi.us.git --branch gh-pages ./dist"
      },
      "foundry-release-git",
      "foundry-release-npm",
      {
        "type": "customCommand",
        "updateFiles": "npm run build"
      },
      {
        "type": "customCommand",
        "updateFiles": "cd dist && git add --all && git commit -m \"Release $FOUNDRY_VERSION\" && git push"
      }
    ]
  },
  "dependencies": {
    "babel-polyfill": "^6.13.0",
    "change-case": "^3.0.0",
    "cuid": "^1.3.8",
    "debug": "^2.2.0",
    "eventsource-polyfill": "^0.9.6",
    "material-design-icons": "^3.0.1",
    "material-design-lite": "^1.2.1",
    "materialize-css": "^0.97.7",
    "tinydown": "^0.1.9",
    "vue": "^2.0.1",
    "vue-material-components": "^0.3.4",
    "vue-resource": "^1.0.3",
    "vue-router": "^2.0.0",
    "vue-server-renderer": "^2.0.1",
    "vue-template-compiler": "^2.0.1",
    "vuex": "^2.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.0",
    "autoprefixer-loader": "^3.2.0",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.7.4",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-lodash": "^2.2.1",
    "babel-plugin-syntax-flow": "^6.13.0",
    "babel-plugin-transform-flow-comments": "^6.17.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2016": "^6.16.0",
    "babel-preset-es2017": "^6.16.0",
    "babel-register": "^6.11.5",
    "babel-runtime": "^6.11.6",
    "bourbon": "^4.2.7",
    "bourbon-neat": "^1.8.0",
    "connect-history-api-fallback": "^1.3.0",
    "cross-env": "^1.0.7",
    "cross-spawn": "^2.1.5",
    "css-loader": "^0.25.0",
    "eslint": "^3.7.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-friendly-formatter": "^1.2.2",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-flowtype": "^2.19.0",
    "eslint-plugin-html": "^1.3.0",
    "eslint-plugin-promise": "^1.0.8",
    "eslint-plugin-standard": "^1.3.2",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "foundry": "^4.3.2",
    "foundry-release-require-branch": "0.0.2",
    "function-bind": "^1.0.2",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.22.0",
    "inject-loader": "^3.0.0-beta1",
    "isparta-loader": "^2.0.0",
    "jasmine-core": "^2.4.1",
    "json-loader": "^0.5.4",
    "json-server": "^0.8.21",
    "jsonfile": "^2.3.1",
    "karma": "^0.13.22",
    "karma-coverage": "^0.5.5",
    "karma-jasmine": "^0.3.8",
    "karma-mocha": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-requirejs": "^0.2.6",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.24",
